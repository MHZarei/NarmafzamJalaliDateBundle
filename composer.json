{
  "name" : "mhzarei/jalali-date-bundle",
  "type" : "symfony-bundle",
  "description" : "A symfony 5 bundle to handle Jalali date including a date picker",
  "keywords" : ["Symfony jalali shamsi khorshidi persian date picker bundle"],
  "license": "MIT",
  "authors" : [
    {
      "name" : "Peyman Mohamadpour",
      "email" : "peyman@narmafzam.com"
    },
     {
      "name" : "MH Zarei"
    }
  ],
  "require" : {
    "php": "^5.5.9 || ^7.0",
    "ext-intl": "*"
  },
  "autoload" : {
    "exclude-from-classmap": ["/Tests/"],
    "psr-4": {
      "Narmafzam\\JalaliDateBundle\\" : ""
    }
